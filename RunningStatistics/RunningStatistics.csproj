<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>0.1.0</Version>
    <Company>Scientific Games</Company>
    <Description>A library of online algorithms for statistical measures.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/adknudson/RunningStatistics</PackageProjectUrl>
    <RepositoryUrl>https://github.com/adknudson/RunningStatistics</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Statistics</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
