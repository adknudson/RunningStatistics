<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>0.3.1</PackageVersion>
		<Authors>Alex Knudson</Authors>
		<Title>Running Statistics</Title>
		<Description>A library of single pass algorithms for statistical measures.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Copyright>Copyright (c) Alex Knudson 2023</Copyright>
		<PackageProjectUrl>https://github.com/adknudson/RunningStatistics</PackageProjectUrl>
		<RepositoryUrl>https://github.com/adknudson/RunningStatistics</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>statistics, online algorithm, streaming, single pass</PackageTags>
		<LangVersion>11</LangVersion>
		<Nullable>enable</Nullable>
		<TargetFrameworks>net7.0;net7.0-windows</TargetFrameworks>
	</PropertyGroup>

</Project>
